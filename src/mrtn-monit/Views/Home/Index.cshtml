@model Mrtn.Models.IndexModel
<div class="page-header">
  <h1>List of buildings</h1>
</div>
<div class="list-group">
  @foreach (var building in Model.Buildings)
  {
    <a href="~/@building" class="list-group-item">
      <span class="badge"><span class="glyphicon glyphicon-triangle-right" aria-hidden="true"></span></span>
      @("K" + building)
    </a>
  }
</div>